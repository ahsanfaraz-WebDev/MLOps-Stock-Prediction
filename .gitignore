# Python
__pycache__/
*.py[cod]
*$py.class
*.so

# Virtual Environments
env/
venv/
.env

# AWS Credentials (DO NOT COMMIT)
.env
*.env
aws-credentials.json

# Service Account JSON files (DO NOT COMMIT)
*-service-account.json
*.json
!package.json
!package-lock.json

# Data (don't commit large files directly)
data/raw/*
data/processed/*
# IMPORTANT: Allow .dvc metadata files to be tracked (DVC needs these)
!data/processed/*.dvc
!data/raw/*.dvc
*.csv
!*.dvc
*.parquet

# Models (DVC will track these)
models/*.pkl
models/*.joblib

# Airflow
airflow/logs/
airflow/airflow.db
airflow/airflow.cfg
airflow-webserver.pid
webserver_config.py
airflow.cfg
dags/
logs/
plugins/

# Docker
.docker/
docker-compose.override.yml

# OS
.DS_Store
Thumbs.db

# IDE
.vscode/
.idea/

# DVC - Ignore actual data and models (DVC will track them via .dvc files)
data/raw/*.csv
data/processed/*.csv
# IMPORTANT: Allow .dvc files (metadata files) to be tracked by git
!*.dvc
models/*.pkl
models/*.joblib

# DVC Files to Commit
!.dvc/
!.dvc/config
!.dvc/.gitignore
!.dvc/plots
# Allow all .dvc metadata files to be tracked
!**/*.dvc

# Ignore DVC cache and temp
.dvc/cache/
.dvc/tmp/
